    "build:types": "tsc -p src/tsconfig.types.json",
    "clean": "rm -rf coverage/ dist/",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(ts|js|jsx)\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "docs-readme": "npm run docs && npm run readme",
    "install:link-deps": "npm install && npm link @adobe/cq-spa-page-model-manager && npm link @adobe/cq-spa-component-mapping",
    "linter": "eslint --ext .js,.jsx,.ts,.tsx . && tslint --project tsconfig.json",
    "linter:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix && tslint --project tsconfig.json --fix",
    "postversion": "git push origin && git push origin --tags",
    "preversion": "npm run test && npm run build:production && npm run docs-readme",
    "readme": "node node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "test": "jest --clearCache && jest",
    "test:coverage": "jest --clearCache &&  jest --coverage",
    "test:debug": "jest --coverage --watchAll",
    "tmprelease": "node release.js",
    "version": "npm run docs-readme && git add DOCUMENTATION.md README.md package.json"
